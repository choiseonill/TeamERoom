<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	
	
	<bean id="testController" class="com.TeamERoom.test.web.TestController"></bean>
	
	<bean id="memberController" class="com.TeamERoom.member.web.MemberController">
		<property name="memberService" ref="memberService"/>
	</bean>
	
	<bean id="bookController" class="com.TeamERoom.book.web.BookController">
		<property name="bookService" ref="bookService"/>
	</bean>
	
	<bean id="roomController" class="com.TeamERoom.room.web.RoomController">
		<property name="roomService" ref="roomService"/>
	</bean>




</beans>
